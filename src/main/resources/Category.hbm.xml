<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.Category" table="Category" schema="webshop1">
        <id name="cid">
            <column name="cid" sql-type="varchar(32)"></column>
        </id>
        <property name="cname">
            <column name="cname" sql-type="varchar(20)"/>
        </property>
    </class>
</hibernate-mapping>